{
  "$ref": "#/definitions/ResumeData",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ResumeData": {
      "additionalProperties": false,
      "properties": {
        "introduction": {
          "$ref": "#/definitions/ResumeIntroductionData"
        },
        "profile": {
          "$ref": "#/definitions/ResumeProfileData"
        },
        "techStack": {
          "items": {
            "$ref": "#/definitions/ResumeTechStackGroup"
          },
          "type": "array"
        }
      },
      "required": [
        "profile",
        "introduction",
        "techStack"
      ],
      "type": "object"
    },
    "ResumeIntroductionData": {
      "additionalProperties": false,
      "properties": {
        "contents": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "contents"
      ],
      "type": "object"
    },
    "ResumeProfileData": {
      "additionalProperties": false,
      "properties": {
        "birthDate": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "github": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "birthDate",
        "email",
        "phone",
        "github"
      ],
      "type": "object"
    },
    "ResumeTechStackGroup": {
      "additionalProperties": false,
      "properties": {
        "items": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "title": {
          "type": "string"
        }
      },
      "required": [
        "title",
        "items"
      ],
      "type": "object"
    }
  }
}
